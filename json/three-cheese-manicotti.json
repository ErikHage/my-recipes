  "steps": [
    {
      "id": 1,
      "text": "Preheat over to 400 degrees."
    },
    {
      "id": 2,
      "text": "Cook Manicotti pasta al dente (about 7 minutes in salted boiling water)."
    },
    {
      "id": 3,
      "text": "Drain pasta and rinse in cold water, then set aside in one layer on a sheet pan."
    },
    {
      "id": 4,
      "text": "In a large bowl, stir together ricotta, one cup of the mozzarella, parmesan, egg, parsley, basil, salt, pepper, and nutmeg until thoroughly combined."
    },
    {
      "id": 5,
      "text": "Evenly spread some of the tomato sauce on the bottom of a 13\" x 9\" baking dish."
    },
    {
      "id": 6,
      "text": "Pipe cheese mixture into the pasta shells, and set them into the baking dish in one layer."
    },
    {
      "id": 7,
      "text": "Spread remaining tomato sauce over the pasta."
    },
    {
      "id": 8,
      "text": "Evenly sprinkle the remaining mozzarella cheese over the pasta."
    },
    {
      "id": 9,
      "text": "Cover with foil and bake for 30 minutes."
    },
    {
      "id": 10,
      "text": "Remove foil and bake an additional 5 minutes, or until sauce starts to bubble."
    },
    {
      "id": 11,
      "text": "Garnish with some parsley, basil, and parmesan cheese."
    },
    {
      "id": 12,
      "text": "Serve hot!"
    }
  ],
