  "steps": [
    {
      "id": 0,
      "text": "Preheat oven to 400°F"
    },
    {
      "id": 1,
      "text": "Line baking sheet with foil or parchment paper"
    },
    {
      "id": 2,
      "text": "In a small bowl, whisk together olive oil, lemon juice, garlic, dill, parsley, and paprika"
    },
    {
      "id": 3,
      "text": "Place the trout filets onto the baking sheet, use paper towels to pat the fish dry"
    },
    {
      "id": 4,
      "text": "Brush the olive oil mixture onto both sides of the trout, then season both sides with salt and pepper"
    },
    {
      "id": 5,
      "text": "Leave the fish skin side down for baking"
    },
    {
      "id": 6,
      "text": "If desired, place thin lemon slices on top of the fish"
    },
    {
      "id": 7,
      "text": "Bake 10-12 minutes, until the trout internal temperature reaches 140°F to 145°F"
    }
  ],
