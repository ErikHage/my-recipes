  "steps": [
    {
      "id": 1,
      "text": "Peel and cut potatoes into desire sized wedges, then soak in cool water for 2 minutes."
    },
    {
      "id": 2,
      "text": "Heat oil in pan on medium heat."
    },
    {
      "id": 3,
      "text": "Put two wedges in pan, move them around and watch for when they start to fry."
    },
    {
      "id": 4,
      "text": "Put remaining wedges into pan, and sprinkle with herbs and lemon juice."
    },
    {
      "id": 5,
      "text": "Fry until golden brown, stirring and flipping as you go."
    },
    {
      "id": 6,
      "text": "Place on paper towels to soak up excess oil."
    },
    {
      "id": 7,
      "text": "Sprinkle with salt and pepper."
    }
