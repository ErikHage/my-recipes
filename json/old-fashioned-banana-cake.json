  "steps": [
    {
      "id": 1,
      "text": "Preheat oven to 350F"
    },
    {
      "id": 2,
      "text": "Line 8\" x 8\" square baking pan with parchment paper"
    },
    {
      "id": 3,
      "text": "In a mixing bowl, beat bananas, brown sugar, and granulated sugar until smooth"
    },
    {
      "id": 4,
      "text": "Add the vegetable oil, eggs, sour cream, and 1 teaspoon vanilla extract and beat well"
    },
    {
      "id": 5,
      "text": "In another bowl, combine the flour, baking powder, salt, and cinnamon"
    },
    {
      "id": 6,
      "text": "Slowly add dry ingredients to the wet, folding the dry in (do not over mix)"
    },
    {
      "id": 7,
      "text": "Fold walnuts into the batter"
    },
    {
      "id": 8,
      "text": "Pour batter into the pan"
    },
    {
      "id": 9,
      "text": "Bake 30-35 minutes, until inserted toothpick comes out clean"
    },
    {
      "id": 10,
      "text": "Remove from the oven and allow to cool completely before frosting"
    },
    {
      "id": 11,
      "text": "Combine butter, cream cheese, and 0.5 teaspoon vanilla in a bowl and mix until smooth"
    },
    {
      "id": 12,
      "text": "Add icing sugar and beat only until smooth (do not over beat)"
    },
    {
      "id": 13,
      "text": "Frost the cake, cut into 2\" x 2\" pieces"
    }
  ],
