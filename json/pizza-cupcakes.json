  "steps": [
    {
      "id": 1,
      "text": "Preheat oven to 400F."
    },
    {
      "id": 2,
      "text": "Spray 8 cups of a muffin tin with cooking spray."
    },
    {
      "id": 3,
      "text": "Stretch out pizza dough onto a lightly floured surface, and cut out 8 4\" rounds of dough."
    },
    {
      "id": 4,
      "text": "Place each piece of dough in a muffin cup."
    },
    {
      "id": 5,
      "text": "Add 1 tsp of cheese to each cup, then 1 Tbsp of sauce, then top with 1 Tbsp of cheese."
    },
    {
      "id": 6,
      "text": "Add any additional toppings."
    },
    {
      "id": 7,
      "text": "Bake 15 minutes, until cheese is bubbling and melted."
    },
    {
      "id": 8,
      "text": "Let sit for 5 minutes before serving."
    },
    {
      "id": 9,
      "text": "Extra can be frozen and re-heated."
