  "steps": [
    {
      "id": 1,
      "text": "Preheat oven to 350Â°F"
    },
    {
      "id": 2,
      "text": "Line a 12 muffin pan with paper cups"
    },
    {
      "id": 3,
      "text": "Cream the margarine in a bowl until light and fluffy"
    },
    {
      "id": 4,
      "text": "Add caster sugar, self-rising flour, eggs, baking powder, ground almonds, and milk and mix until well combined"
    },
    {
      "id": 5,
      "text": "Spoon mixture into the muffin pan and bake 15-20 minutes (or until a toothpick inserted comes out clean)"
    },
    {
      "id": 6,
      "text": "Remove form oven and cool for 10 minutes"
    },
    {
      "id": 7,
      "text": "For icing, mix the icing sugar and lemon juice in a bowl until smooth"
    },
    {
      "id": 8,
      "text": "Using an apple corer, carefully remove the middle of the cupcakes"
    },
    {
      "id": 9,
      "text": "Fill the holes with raspberry jam"
    },
    {
      "id": 10,
      "text": "Carefully spoon icing onto the top of each cupcake until it reaches the sides of the cup"
    },
    {
      "id": 11,
      "text": "Top with a candied cherry"
    }
