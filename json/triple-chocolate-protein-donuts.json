  "steps": [
    {
      "id": 1,
      "text": "Preheat oven to 325 degrees F."
    },
    {
      "id": 2,
      "text": "Blend dates and 0.25 cups of almond slices in a food processor until crumbled."
    },
    {
      "id": 3,
      "text": "Add almond flour, coconut flour, baking powder, cocoa, and protein powder to a large mixing bowl, whisk thoroughly."
    },
    {
      "id": 4,
      "text": "Add date mix, eggs, and milk to bowl and mix until completely blended (like brownie batter)."
    },
    {
      "id": 5,
      "text": "Spoon into donut pan, smooth tops."
    },
    {
      "id": 6,
      "text": "Bake for 8-10 minutes (toothpick check)."
    },
    {
      "id": 7,
      "text": "Let cool completely before adding topping."
    },
    {
      "id": 8,
      "text": "For the topping:"
    },
    {
      "id": 9,
      "text": "Crush remaining 1/8 of a cup of almonds gently."
    },
    {
      "id": 10,
      "text": "Microwave chocolate squares for 30 seconds at a time, until soft."
    },
    {
      "id": 11,
      "text": "Dip donuts into chocolate, then almonds, one at a time."
    },
    {
      "id": 12,
      "text": "Allow chocolate topping to cool and set."
    }
  ],
