  "steps": [
    {
      "id": 1,
      "text": "Preheat oven to 325F."
    },
    {
      "id": 2,
      "text": "Mix together flour, cinnamon, baking powder, and salt. Set aside."
    },
    {
      "id": 3,
      "text": "Cream butter and sugar in electric mixer until smooth and light in color."
    },
    {
      "id": 4,
      "text": "Mix eggs in one at a time on low speed until incorporated."
    },
    {
      "id": 5,
      "text": "Mixing on low speed, add 1/3 of the dry mix, then 1/2 of the milk, repeat until all ingredients incorporated."
    },
    {
      "id": 6,
      "text": "Grease and flour a Bundt pan."
    },
    {
      "id": 7,
      "text": "Pour batter into pan and bake for 75 minutes."
    },
    {
      "id": 8,
      "text": "Remove and cool for 10-15 minutes before removing from the pan."
    }
