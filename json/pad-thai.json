  "steps": [
    {
      "id": 1,
      "text": "Soak rice noodles in hot water until softened (30-45 min)."
    },
    {
      "id": 2,
      "text": "Drain noodles and set aside."
    },
    {
      "id": 3,
      "text": "In a mixing bowl, toss sesame oil, cornstarch, and chicken until coated."
    },
    {
      "id": 4,
      "text": "In a small bowl, whisk together the fish sauce, lemon juice, and sugar, set aside."
    },
    {
      "id": 5,
      "text": "Preheat wok or large skillet and pour in half the peanut oil."
    },
    {
      "id": 6,
      "text": "Add chicken and cook until cooked through (~2 min)."
    },
    {
      "id": 7,
      "text": "Remove from wok/skillet and set aside."
    },
    {
      "id": 8,
      "text": "Reheat wok/skillet and pour in the remaining peanut oil,"
    },
    {
      "id": 9,
      "text": "Add garlic and scallions, cook for about one minute."
    },
    {
      "id": 10,
      "text": "Slowly add the eggs, stirring constantly."
    },
    {
      "id": 11,
      "text": "Once eggs are cooked, add the drained noodles, fish sauce mixture, and cooked chicken."
    },
    {
      "id": 12,
      "text": "Toss until combined."
    },
    {
      "id": 13,
      "text": "Transfer to plates and top with bean sprouts, peanuts, lime wedges, and cilantro."
    },
    {
      "id": 14,
      "text": "Serve immediately."
    }
  ],
