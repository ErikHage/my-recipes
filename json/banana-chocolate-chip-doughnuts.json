  "steps": [
    {
      "id": 1,
      "text": "Preheat oven to 325 degrees"
    },
    {
      "id": 2,
      "text": "Combine mashed bananas, sugar, and yogurt in an electric mixer"
    },
    {
      "id": 3,
      "text": "Add melted butter, eggs, and vanilla to mixer and combine"
    },
    {
      "id": 4,
      "text": "Add flour, baking powder, baking soda, and salt to mixer, mix until just incorporated"
    },
    {
      "id": 5,
      "text": "Using a spatula, fold in chocolate chips"
    },
    {
      "id": 6,
      "text": "Scoop batter into a ziplock bag and seal, removing excess air"
    },
    {
      "id": 7,
      "text": "Snip a corner to create a piping bag"
    },
    {
      "id": 8,
      "text": "Pipe into greased doughnut baking pan, filling each mold 3/4 full"
    },
    {
      "id": 9,
      "text": "Bake 12-15 minutes"
    }
