  "steps": [
    {
      "id": 1,
      "text": "Preheat oven to 375°F"
    },
    {
      "id": 2,
      "text": "Heat cherries and amaretto over medium-low heat until cherries have softened, about 8 minutes"
    },
    {
      "id": 3,
      "text": "Drain, reserving 2 Tbsp. of liquid (add more amaretto if less liquid)"
    },
    {
      "id": 4,
      "text": "Sift together flour, baking powder, and salt into a bowl"
    },
    {
      "id": 5,
      "text": "Put butter and granulated sugar in an electric mixer with paddle attachment"
    },
    {
      "id": 6,
      "text": "Mix on medium speed until fluffy, about 2 minutes"
    },
    {
      "id": 7,
      "text": "Mix in 3 whole eggs, one at a time, until incorporated"
    },
    {
      "id": 8,
      "text": "Mix in reserved cherry liquid and the vanilla extract"
    },
    {
      "id": 9,
      "text": "Reduce speed to low, and gradually mix in flour mixture"
    },
    {
      "id": 10,
      "text": "Stir in cherries and almonds by hand"
    },
    {
      "id": 11,
      "text": "On a lightly floured surface, halve the dough into two logs"
    },
    {
      "id": 12,
      "text": "Shape each half into a 12.5\" x 2.5\" log"
    },
    {
      "id": 13,
      "text": "Flatten logs to 0.5\" thick"
    },
    {
      "id": 14,
      "text": "Transfer to a baking sheet lined with parchment paper"
    },
    {
      "id": 15,
      "text": "Brush logs with beaten egg, sprinkle with sanding sugar"
    },
    {
      "id": 16,
      "text": "Bake 35 minutes, rotating baking sheets halfway though the bake"
    },
    {
      "id": 17,
      "text": "Transfer to wire racks to cool, about 20 minutes"
    },
    {
      "id": 18,
      "text": "Reduce oven temp to 300°F"
    },
    {
      "id": 19,
      "text": "Cut each log on a diagonal into 16-18 pieces"
    },
    {
      "id": 20,
      "text": "Transfer pieces onto racks, laying them on their sides"
    },
    {
      "id": 21,
      "text": "Set racks on baking sheets, bake 8 minutes"
    },
    {
      "id": 22,
      "text": "Flip and bake 8 more minutes"
    },
    {
      "id": 23,
      "text": "Let cool until crisp"
    }
  ],
